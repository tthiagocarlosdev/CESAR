programa
{
	
	funcao inicio()
	{
		inteiro a, b, soma

		escreva("Digite o 1º valor: ")
		leia(a)
		escreva("Digite o 2º valor: ")
		leia(b)
		soma = a + b
		limpa()
		escreva("SOMA = ", soma,"\n")
	}
}
